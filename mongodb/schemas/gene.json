{
	"gene": {
		"validator": {
			"$jsonSchema": {
				"type": "object",
				"required": [
					"_id",
					"organism_id"
				],
				"properties": {
					"_id": {
						"type": "string",
						"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
					},
					"bnumber": {
						"type": "string"
					},
					"fragments": {
						"type": "array",
						"uniqueItems": true,
						"items": {
							"type": "object",
							"required": [
								"strand",
								"leftEndPosition",
								"rightEndPosition"
							],
							"properties": {
								"strand": {
									"type": "string",
									"enum": [
										"forward",
										"reverse"
									]
								},
								"leftEndPosition": {
									"type": "number",
									"multipleOf": 1.0
								},
								"rightEndPosition": {
									"type": "number",
									"multipleOf": 1.0
								},
								"sequence": {
									"type": "string",
									"pattern": "^[ATCGatcg]+$"
								},
								"centisomePosition": {
									"type": "number"
								}
							},
							"additionalProperties": false
						}
					},
					"gcContent": {
						"type": "number"
					},
					"leftEndPosition": {
						"type": "number",
						"multipleOf": 1.0
					},
					"name": {
						"type": "string"
					},
					"organism_id": {
						"type": "string",
						"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
					},
					"rightEndPosition": {
						"type": "number",
						"multipleOf": 1.0
					},
					"sequence": {
						"type": "string",
						"pattern": "^[ATCGatcg]+$"
					},
					"strand": {
						"type": "string",
						"enum": [
							"forward",
							"reverse"
						]
					}
				},
				"additionalProperties": false
			}
		},
		"validationLevel": "strict",
		"validationAction": "error"
	}
}